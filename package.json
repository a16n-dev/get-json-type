{
  "name": "get-json-type",
  "version": "1.2.0",
  "description": "Does what it says on the tin. Generates a Typescript type from arbitrary JSON",
  "keywords": [
    "typescript",
    "json",
    "type",
    "generator",
    "schema"
  ],
  "author": "Alex Nicholson <alex@a16n.dev>",
  "engines": {
    "node": ">=22.19.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/a16n-dev/get-json-type",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a16n-dev/get-json-type.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
